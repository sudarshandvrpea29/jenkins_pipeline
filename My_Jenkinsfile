pipeline {
agent any
stages {
stage('BUID'){
steps{
echo "This is the build stage"

}
}

stage('TEST PARALLEL'){
parallel {
stage('TEST ON CHROME') {
steps{
echo "This is the TEST on CHROME BROWSER"
sh 'sleep 5;
}
}
stage('TEST ON SAFARI') {
steps{
echo "This is the TEST on SAFARI BROWSER"
sh 'sleep 5;
}
}
}
}

stage('DEPLOYED'){
parallel {
stage ('server 1') {
steps{
echo "This is the deployed stage1"
}
}
stage ('server 2') {
steps{
echo "This is the deployed stage2"
}
}
stage ('server 3') {
steps{
echo "This is the deployed stage3"
}
}
}
}
}
}
